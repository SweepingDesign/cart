{
    "name": "leaphly/cart",
    "type": "library",
    "description": "high quality php shopping Cart.",
    "keywords": ["shopping cart management"],
    "homepage": "http://leaphly.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Giulio De Donato",
            "email": "liuggio@gmail.com"
        },
        {
            "name": "Leaphly Community",
            "homepage": "https://github.com/leaphly/CartBundle/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "doctrine/common": "2.4.x-dev",
        "symfony/validator": "~2.4",
        "symfony/event-dispatcher": "~2.4",
        "symfony/http-kernel": "~2.4",
        "symfony/form": "~2.1",
        "jms/serializer": "@stable",
        "pugx/godfather": "dev-master",
        "yohang/finite": "1.0.0-BETA2"
    },
    "repositories": [{
        "type": "package",
        "package": {
            "name": "yohang/finite",
            "version": "1.0.0-BETA2",
            "dist": {
                "type": "zip",
                "url": "https://github.com/liuggio/Finite/zipball/master"
            },
            "require": {
                "php":                      ">=5.3.0",
                "symfony/options-resolver": ">=2.1,<3.0",
                "symfony/event-dispatcher": ">=2.1,<3.0"
            },
            "autoload": {
                "psr-0": { "Finite": "src/" }
            },
            "config": {
                "bin-dir":"bin"
            }
        }
    }],
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "suggest": {
        "doctrine/orm": "Required when using ORM as db-driver",
        "doctrine/odm": "Required when using ODM as db-driver"
    },
    "autoload": {
        "psr-0": { "Leaphly\\Cart\\": "src/" }
    }
}
